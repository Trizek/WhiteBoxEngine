////////////////////////////////////////////////////////////////////////////////
// Filename: color.ps
////////////////////////////////////////////////////////////////////////////////


/////////////////////
// INPUT VARIABLES //
/////////////////////
in lowp vec3 outColor;
in lowp vec2 texCoord;
in lowp vec3 outNormal;


//////////////////////
// OUTPUT VARIABLES //
//////////////////////
out lowp vec4 outputColor;

uniform sampler2D 	shaderTexture;

////////////////////////////////////////////////////////////////////////////////
// Pixel Shader
////////////////////////////////////////////////////////////////////////////////
void main(void)
{

	lowp vec4 texturColor = texture(shaderTexture, texCoord); 
	outputColor = texturColor;
}
